	<PackageReference Include="Throw" Version="1.3.1" />
  </ItemGroup>

  <ItemGroup>
    <EditorConfigFiles Remove="G:\GitHub-Projects\FlutterBootstrapper\FlutterBootstrapper\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <None Include="G:\GitHub-Projects\FlutterBootstrapper\FlutterBootstrapper\.editorconfig" />
  </ItemGroup>

